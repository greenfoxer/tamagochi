    "nodemon": "^3.1.9",
    "prisma": "^6.5.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@hapi/hapi": "^21.3.0",
    "@prisma/client": "^6.5.0",
    "dotenv": "^16.4.0",
    "node-telegram-bot-api": "^0.66.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
